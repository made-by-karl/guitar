{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/icons/*.png"
        ]
      }
    },
    {
      "name": "samples",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/samples/**/*.mp3"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "chord-data",
      "urls": [
        "/api/chords"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "10s"
      }
    }
  ]
}
